{
  "name": "GPT Question Navigator",
  "description": "Show a clickable list of user questions on ChatGPT web.",
  "version": "0.1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*",
        "https://*.openai.com/*",
        "https://gemini.google.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
